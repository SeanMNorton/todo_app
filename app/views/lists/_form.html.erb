<%= form_for list do |f| %>

  <%= f.label :title, class: "control-label" %>
  <%= f.text_field :title, class: "form-control" %>

  <%= f.label :description, class: "control-label" %>
  <%= f.text_field :description, class: "form-control" %>

  <%= f.submit "Save / Edit", class: "btn btn-primary" %>
<% end %>
